{
    "name": "makeradmin-admin",
    "version": "1.0.0",
    "description": "See repository",
    "main": "webpack.config.js",
    "repository": "https://github.com/makerspace/makeradmin",
    "license": "GPL-3.0",
    "engines": {
        "npm": ">=10.5.2",
        "node": ">=20"
    },
    "dependencies": {
        "@types/colorbrewer": "^1.0.32",
        "chart.js": "4.4",
        "chartjs-adapter-luxon": "^1.3.1",
        "chartjs-plugin-annotation": "^3.1.0",
        "chartjs-plugin-zoom": "^2.2.0",
        "classnames": "^2.3.1",
        "colorbrewer": "^1.5.9",
        "html5-qrcode": "^2.3.8",
        "luxon": "^3.5.0",
        "prop-types": "^15.7.2",
        "react": "^18.0.0",
        "react-day-picker": "^9.5",
        "react-dom": "^18.0.0",
        "react-router": "^7.9",
        "react-router-dom": "^7.9",
        "react-select": "^5.9",
        "uikit": "^3.22.0",
        "underscore": "^1.13.0"
    },
    "devDependencies": {
        "@babel/core": "^7.13.15",
        "@babel/eslint-parser": "^7.13.14",
        "@babel/plugin-transform-runtime": "^7.25.9",
        "@babel/preset-env": "^7.13.15",
        "@babel/preset-react": "^7.13.13",
        "@babel/preset-typescript": "^7.26.0",
        "@babel/register": "^7.13.14",
        "@babel/runtime": "^7.26.0",
        "@jest/globals": "^29.7.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/uikit": "^3.14.5",
        "@types/underscore": "^1.11.1",
        "babel-core": "^7.0.0-0",
        "babel-jest": "^29.7.0",
        "babel-loader": "^8.2.2",
        "css-loader": "^6.7.3",
        "eslint": "^7.24.0",
        "eslint-plugin-react": "^7.23.2",
        "jest": "^29.7.0",
        "react-test-renderer": "^18.0.0",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.4.2",
        "typescript": "^5.9.2",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0"
    },
    "scripts": {
        "dev": "./node_modules/.bin/webpack-dev-server --mode development",
        "build": "./node_modules/.bin/webpack",
        "test": "jest",
        "eslint": "eslint src --ext .js --ext .jsx"
    },
    "author": "",
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react",
            "@babel/preset-typescript"
        ],
        "plugins": [
            "@babel/plugin-transform-runtime"
        ]
    },
    "jest": {
        "setupFiles": [
            "<rootDir>/jestSetup.js"
        ]
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "node": true,
            "es6": true
        },
        "globals": {
            "config": true,
            "__BUILD_DATE__": "readonly",
            "__COMMIT_HASH__": "readonly"
        },
        "ignorePatterns": [
            "/src/**/__test__/*"
        ],
        "parser": "@babel/eslint-parser",
        "plugins": [
            "react"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended"
        ],
        "parserOptions": {
            "ecmaVersion": 8,
            "sourceType": "module",
            "ecmaFeatures": {
                "jsx": true,
                "experimentalObjectRestSpread": true
            }
        },
        "rules": {
            "strict": "off",
            "no-unused-vars": [
                "error",
                {
                    "varsIgnorePattern": "styles"
                }
            ],
            "no-unsafe-negation": "warn",
            "array-callback-return": "warn",
            "eqeqeq": "warn",
            "no-unused-expressions": "warn",
            "no-with": "warn",
            "no-shadow": "warn",
            "no-undefined": "warn",
            "no-use-before-define": [
                "warn",
                {
                    "functions": false,
                    "classes": false
                }
            ],
            "wrap-iife": "error",
            "no-var": "error",
            "no-proto": "error",
            "no-throw-literal": "error",
            "no-implicit-globals": "error",
            "no-global-assign": "error",
            "no-eval": "error",
            "no-else-return": "error",
            "consistent-return": "error",
            "block-scoped-var": "error",
            "no-constant-condition": "off",
            "semi": "error",
            "react/prop-types": "off",
            "react/display-name": "off"
        },
        "settings": {
            "react": {
                "version": "detect"
            }
        }
    }
}
